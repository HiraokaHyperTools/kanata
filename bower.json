{
  "name": "kanata",
  "description": "Kanata is one of the Bootstrap original theme.",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "japanese",
    "theme",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "author": "logue and kenjiuno",
  "homepage": "https://logue.be/",
  "repository": {
    "type": "git",
    "url": "git://github.com/logue/Kanata.git"
  },
  "license": "MIT",
  "moduleType": "globals",
  "main": [
    "dist/js/bootstrap.js",
    "dist/css/bootstrap.css"
  ],
  "ignore": [
    "/.*",
    "dist/assets",
    "dist/*.html",
    "src",
    "CNAME"
  ],
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7"
  },
  "exportsOverride": {
    "bootstrap-sass": {},
    "bootstrap": {
      "js": "dist/js/bootstrap.**js",
      "fonts": "dist/fonts/**/*"
    },
    "jquery": {}
  }
}
